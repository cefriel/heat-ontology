}, {
  "@id" : "https://w3id.org/heat#Construct",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "A Construct is a concept defined to represent specific attributes in a given context. A construct is defined by a series of questions used to measure and evaluate these specific aspects quantitatively."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Construct"
  } ]
}, {
  "@id" : "https://w3id.org/heat#Context",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "A Context is the specific environment, application domain, or scenario in which a tool or a construct is applied, providing the situational framework that influences how concepts are interpreted, measured, and utilized."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Context"
  } ]
}, {
  "@id" : "https://w3id.org/heat#GeneralConstruct",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "A General Construct is a generalised concept that serves as a reference point for aggregating all related constructs from different tools, enabling the systematic organisation and comparison of equivalent constructs in different contexts."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "General Construct"
  } ]
}, {
  "@id" : "https://w3id.org/heat#LikertAnswer",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "A Likert Answer is a subjective response format in which a user selects a point on an ordered categorical scale to indicate the intensity or extent of his/her perception, evaluation, or experience regarding a given item."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Likert Answer"
  } ]
}, {
  "@id" : "https://w3id.org/heat#ReferenceTheory",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "A Reference Theory is a standardized, conceptual theory that aggregates related tools, models, or variants under a unified theoretical structure. It serves as the canonical reference point to which all specific implementations or adaptations (tools/models) are linked."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Reference Theory"
  } ]
}, {
  "@id" : "https://w3id.org/heat#Tool",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "A Tool is a structured conceptual system that explains phenomena by specifying key constructs and the relationships between them; each tool is defined by a set of constructs, which are defined through a series of measurement elements (questions) that allow for an objective evaluation of the phenomenon analysed by the tool."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Tool"
  } ]
