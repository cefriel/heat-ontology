    <rdfs:domain rdf:resource="https://w3id.org/heat#Construct"/>
    <rdfs:range rdf:resource="https://w3id.org/heat#GeneralConstruct"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/heat#hasLikertAnswer">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Question to its Likert Answer.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has likert answer</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/survey-ontology#Question"/>
    <rdfs:range rdf:resource="https://w3id.org/heat#LikertAnswer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/heat#textualValueRange">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property represents an ordered textual range in which a Likert Answer is defined.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>textual value range</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/heat#LikertAnswer"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/heat#isOriginal">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property represents whether a tool is validated as a standard, i.e. whose constructs and questions have not been defined based on another reference tool, or not.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>is original</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/heat#Tool"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/heat#LikertAnswer">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A Likert Answer is a subjective response format in which a user selects a point on an ordered categorical scale to indicate the intensity or extent of his/her perception, evaluation, or experience regarding a given item.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Likert Answer</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#versionIRI">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/heat#hasContext">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Tool to its Context.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has context</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/heat#Tool"/>
    <rdfs:range rdf:resource="https://w3id.org/heat#Context"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/heat#refersToTheory">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>refers to theory</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/heat#Tool"/>
    <rdfs:range rdf:resource="https://w3id.org/heat#ReferenceTheory"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/heat#isScaleReversed">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property represents a Likert Answer whose meaning is reversed compared to the meaning of a construct, in the measurement or evaluation of a specific aspect.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>is scale reversed</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/heat#LikertAnswer"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/heat#GeneralConstruct">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A General Construct is a generalised concept that serves as a reference point for aggregating all related constructs from different tools, enabling the systematic organisation and comparison of equivalent constructs in different contexts.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>General Construct</rdfs:label>
  </rdf:Description>
