  <rdf:Description rdf:about="https://w3id.org/heat#numericalValueRange">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property represents an ordered numerical range in which a Likert Answer is defined.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>numerical value range</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/heat#LikertAnswer"/>
